{
  "name": "koitoror-university (react/node)",
  "version": "0.1.0",
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.13" 
  },
  "private": true,
  "proxy": "https://koitoror-university.herokuapp.com",
  "dependencies": {
    "antd": "^3.26.19",
    "axios": ">=0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "heroku": "^7.56.0",
    "http": "0.0.0",
    "nodemon": "^2.0.11",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "serve": "^12.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "cd ./frontend && react-scripts start",
    "postbuild": "cd ./frontend && npm run build",
    "build": "npm install --prefix frontend && npm run build --prefix frontend",
    "test": "cd ./frontend && react-scripts test --env=jsdom",
    "eject": "cd ./frontend && react-scripts eject",
    "postinstall": "cd ./frontend && npm run build",
    "heroku-postbuild": "cd ./frontend && npm install --only=dev --no-shrinkwrap && npm run build"
  }
}
