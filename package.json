{
  "name": "koitoror-university",
  "version": "2.1.0",
  "engines": {
    "node": "v17.4.0",
    "npm": "8.3.2"
  },
  "private": true,
  "proxy": "https://koitororuniversity.ml",
  "dependencies": {
    "antd": "^4.18.5",
    "axios": ">=0.25.0",
    "body-parser": "^1.19.1",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "http": "0.0.0",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "serve": "^13.0.2"
  },
  "scripts": {
    "start": "cd ./frontend && node server.js",
    "dev": "cd ./frontend && react-scripts start",
    "postbuilder": "cd ./frontend && npm run build",
    "buildinstaller": "npm install --prefix frontend && npm run build --prefix frontend",
    "test": "cd ./frontend && react-scripts test --env=jsdom",
    "eject": "cd ./frontend && react-scripts eject",
    "heroku-postbuild": "cd ./frontend && npm install --only=prod --no-shrinkwrap && npm run postbuilder"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "redux-devtools-extension": "^2.13.9"
  }
}
